{
  "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Node",
    "lib": ["ESNext", "DOM"],
    "allowJs": true,
    "checkJs": false,
    "jsx": "preserve",
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": ".",
    "paths": {
      "#app": [".nuxt/types/app"],
      "#imports": [".nuxt/types/imports"],
      "#pinia": [".nuxt/types/pinia"],
      "@/*": ["./*"]
    },
    "types": ["@types/node", "nuxt", "@nuxt/types"]
  },
  "include": ["**/*.ts", "**/*.d.ts", "**/*.vue"],
  "exclude": ["node_modules", ".nuxt", "dist"]
}
